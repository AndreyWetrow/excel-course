{
  "parser": "babel-eslint",
  "env": {
    "es6": true,
    "browser": true,
    "node": true
  },
  "rules": {
    "semi": [2, "always", { "omitLastInOneLineBlock": true}],
    "max-len": "off",
    "operator-linebreak": "off",
    "linebreak-style": "off",
    "arrow-parens": "off",
    "comma-dangle": "off",
    "no-console": "off",
    "require-jsdoc": "off",
    "quotes": [0, "double"]
  },
  "extends": ["eslint:recommended", "google"]
}